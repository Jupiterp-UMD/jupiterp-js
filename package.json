{
  "name": "@jupiterp/jupiterp",
  "version": "0.1.0",
  "description": "A TypeScript library for working with UMD course data from the Jupiterp API.",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "jest",
    "test:unit": "jest -w 1 -c jest.config.mjs --selectProjects unit",
    "test:int": "npm run build && jest -w 1 -c jest.config.mjs --selectProjects integration",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": [
    [
      {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    ]
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
